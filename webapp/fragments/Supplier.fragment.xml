<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        noDataText="No Products Found"
        title="Supplier Number"
        search="ParenthandleSearch"
        confirm="selectSupplier"
        liveChange="_ParentValueHelpSearch"
        cancel="handleClose"
        items="{/suppData}"
        id="sup"
    >
        <StandardListItem
            title="{NAME1}"
            description="{LIFNR}"
            icon="sap-icon://sys-add"
            class="sapUiSmallMarginEnd decreaseFont"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
            id="suppData"
        />
    </SelectDialog>
</core:FragmentDefinition>
