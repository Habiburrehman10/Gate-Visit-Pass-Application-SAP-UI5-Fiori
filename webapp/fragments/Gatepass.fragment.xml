<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        noDataText="No Products Found"
        title="Select Gate Pass"
        search="ParenthandleSearch"
        confirm="selectParentItem"
        liveChange="_ParentValueHelpSearch"
        cancel="handleClose"
        items="{/matData}"
        id="gp"
    >
        <StandardListItem
            title="GatePass Number"
            description="{GP_NO}"
            icon="sap-icon://sys-add"
            class="sapUiSmallMarginEnd decreaseFont"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
        
        />
    </SelectDialog>

