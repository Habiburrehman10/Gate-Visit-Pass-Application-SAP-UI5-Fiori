<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        noDataText="No Products Found"
        title="Customer Number"
        search="ParenthandleSearch"
        confirm="selectCustomer"
        liveChange="_ParentValueHelpSearch"
        cancel="handleClose"
        items="{/custData}"
        id="cust"
    >
        <StandardListItem
            title="{NAME1}"
            description="{KUNNR}"
            icon="sap-icon://sys-add"
            class="sapUiSmallMarginEnd decreaseFont"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
            id="custData"
        />
    </SelectDialog>
</core:FragmentDefinition>
